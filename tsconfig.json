{
    "compilerOptions": {
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "forceConsistentCasingInFileNames": true,
      "allowSyntheticDefaultImports": true,
      "jsx": "react",
      "module": "es6",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "outDir": "./build",
      "preserveConstEnums": true,
      "target": "es5",
      "downlevelIteration": true
    },
    "include": ["src"],
    "exclude": [
      "node_modules"
    ]
  }